                      <Assign.Value>
                        <InArgument x:TypeArguments="x:String">
                          <mca:CSharpValue x:TypeArguments="x:String" sap2010:WorkflowViewState.IdRef="CSharpValue`1_32">"SELECT IDVENTAEDS 'No. Recibo sin PPC', CONCAT(IDVENTAEDS,O.CODIGOUNICO) 'Key', NOMBREESTACION 'Venta: Nombre PDV', O.CODIGOUNICO 'Venta: Código Unico PDV', IdDocumento, IdFormaPago, IdIslero, VALOR AS 'Monto', V.FECHA 'Venta: Fecha de Transacción' FROM VNT_VENTAGAS V INNER JOIN EDS_ESTACIONES E ON V.IDESTACION = E.IDESTACION INNER JOIN OT_EDS O ON E.IDESTACION = O.IDESTACION INNER JOIN VNT_PAGOSVENTAGAS P ON V.IDVENTA = P.IDVENTA WHERE V.FECHATURNOVENTA BETWEEN '" + Str_Fecha_Inicio + "' AND '" + Str_Fecha_Fin + "' AND IDFORMAPAGO IN (253,254) AND P.VALOR &gt; 0 ORDER BY V.FECHA;"</mca:CSharpValue>
